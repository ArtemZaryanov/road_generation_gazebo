<launch>
<node name="world_creation" pkg="road_generation_gazebo" type="road_gazebo.py" required="true">
<!--node name="world_creation" pkg="road_generation_gazebo" type="road_gazebo.py"-->
  <rosparam command="load" file="$(find road_generation_gazebo)/yaml/road_metadata.yaml" />
  <param name="show_plot" value="true" />
  <param name="offset_road_add_noise" value="true" />
  <param name="offset_road_lateral_distance" value="5.0" />
  <param name="offset_road_linear_density" value="30.0" />
  <param name="offset_road_noise_var" value="0.5" />
  <param name="model_name" value="bush" />
  <!--param name="model_name" value="cylinder"/-->
</node>
</launch>
